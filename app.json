{
  "expo": {
    "name": "EV-charging-station",
    "slug": "EV-charging-station",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconCharging.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/iconCharging.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconCharging.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.pmaheshwari049.EVchargingstation"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow EV Station app to use your location."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "b3b663ed-6bed-47b4-80f1-86d11269c49c"
      }
    }
  }
}
